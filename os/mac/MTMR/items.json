      /**
   * SPOTIFY
   * - Show current track when Spotify is open
   * - Play / Pause track on touch
   */
      // Shuffle
      {
        "action": "appleScript",
        "actionAppleScript": {
          "inline": "tell application \"Spotify\"\rif shuffling then\rset shuffling to false\relse\rset shuffling to true\rend if\rend tell"
        },
        "align": "right",
        "bordered": false,
        "image": {
          "base64": ""
        },
        "refreshInterval": 2,
        "source": {
          "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rif player state is playing then\rif shuffling is true then\rreturn \"on\"\relse\rreturn \"off\"\rend if\relse\rreturn \"\"\rend if\rend tell\rend if\rreturn \"\""
        },
        "type": "appleScriptTitledButton"
      },
      // Info
      {
        "action": "appleScript",
        "actionAppleScript": {
          "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rplaypause\rend tell\rend if\r"
        },
        "align": "left",
        "autoResize": true,
        "image": {
          "base64": ""
        },
        "longAction": "openUrl",
        "longUrl": "spotify://",
        "refreshInterval": 1,
        "source": {
          "filePath": "~/.dotfiles/os/mac/MTMR/spotify.scpt"
        },
        "type": "appleScriptTitledButton"
      },
      /**
       * NETWORK
